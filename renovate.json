{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "timezone": "America/Los_Angeles",
  "schedule": [
    "before 5am on monday"
  ],
  "enabledManagers": [
    "npm"
  ],
  "baseBranches": [
    "development"
  ],
  "ignorePaths": [
    "public/**",
    "Dockerfile*",
    "docker-compose.yml",
    ".nvmrc"
  ],
}